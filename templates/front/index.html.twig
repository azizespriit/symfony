<!-- Styles CSS -->
<style>
.pagination li:not(.previous):not(.next):not(.disabled):not(.active) {
    display: none;
}

/* Style général de la pagination */
.pagination {
    display: flex;
    list-style: none;
    justify-content: center;
    padding: 0;
    gap: 10px;
}

/* Style des flèches */
.pagination li a,
.pagination li span {
    display: inline-block;
    padding: 10px 15px;
    border: 1px solid #007bff;
    color: #007bff;
    border-radius: 50%;
    font-size: 18px;
    background-color: #fff;
    transition: all 0.3s ease;
}

.pagination li a:hover {
    background-color: #007bff;
    color: white;
}

.pagination .disabled span {
    color: #ccc;
    border-color: #ccc;
    background-color: #f8f9fa;
    cursor: not-allowed;
}

/* Active state (si tu veux le garder) */
.pagination .active span {
    background-color: #007bff;
    color: white;
    border-color: #007bff;
    cursor: default;
}
  .class-item {
    position: relative;
    overflow: hidden;
    border-radius: 10px;
    background-color: #f8f9fa;
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
  }

  .class-item-text {
    padding: 15px;
    color: #333;
    z-index: 2; /* Assure que le texte reste au-dessus de la description */
  }

  .hover-description {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.9); /* Fenêtre semi-transparente */
    color: white;
    padding: 20px;
    border-radius: 10px;
    display: none;
    z-index: 1; /* Positionne la description sous le texte mais au-dessus de l'image */
    text-align: center;
    box-sizing: border-box;
    overflow-y: auto; /* Si le texte est long, il peut défiler */
  }

  .class-item:hover .hover-description {
    display: flex; /* Affiche la description au survol */
  }

  .class-item-thumbnail img {
    max-width: 100%;
    height: auto;
    border-radius: 8px;
  }

  .btn-primary {
    background-color: #007bff;
    border: none;
    padding: 10px 20px;
    color: white;
    border-radius: 5px;
    cursor: pointer;
    transition: background-color 0.3s;
  }

  .btn-primary:hover {
    background-color: #000000;
  }
</style>

<!-- Script JavaScript pour afficher/masquer la description -->
<script>
  document.querySelectorAll('.class-item').forEach(function(item) {
    item.addEventListener('mouseenter', function() {
      var description = item.querySelector('.hover-description');
      description.style.display = 'block';
    });

    item.addEventListener('mouseleave', function() {
      var description = item.querySelector('.hover-description');
      description.style.display = 'none';
    });
  });
</script>
{% if app.session.flashbag.has('success') %}
    <div id="success-notification" class="alert alert-success">
        {{ app.session.flashbag.get('success')|first }}
    </div>
    <style>
        #success-notification {
            position: fixed;
            top: 20%;
            left: 50%;
            transform: translateX(-50%);
            font-size: 1.5rem; /* Augmente la taille du texte */
            padding: 20px;
            width: 80%; /* Vous pouvez ajuster la largeur selon vos préférences */
            max-width: 600px;
            text-align: center;
            border-radius: 10px;
            box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);
            z-index: 1050; /* S'assure que la notification est au-dessus des autres éléments */
            opacity: 1;
            transition: opacity 0.3s ease-in-out;
        }
        
        #success-notification.hide {
            opacity: 0;
            pointer-events: none; /* Empêche l'interaction avec l'élément lorsque masqué */
        }
    </style>
    <script>
        setTimeout(function() {
            var notification = document.getElementById('success-notification');
            notification.classList.add('hide');
        }, 5000); // Masquer la notification après 5 secondes
    </script>
{% endif %}




